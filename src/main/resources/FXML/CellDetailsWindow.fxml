<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.menus.gamepopupmenus.CellDetailsWindowGraphics"
      styleClass="background"
      stylesheets="@../CSS/GamePopUpMenus.css"
      prefHeight="200.0" prefWidth="300.0">

    <ImageView layoutX="260" layoutY="20" onMouseClicked="#exitPopUp">
        <Image url="@../images/menus/XButton.png" requestedWidth="20" requestedHeight="20"/>
    </ImageView>

    <Label layoutX="20" layoutY="20" styleClass="headerLabel" fx:id="headerLabel">
    </Label>

    <Label layoutX="20" layoutY="50" styleClass="textLabel" fx:id="textLabel">
    </Label>

</Pane>
