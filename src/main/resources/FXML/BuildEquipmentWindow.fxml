<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="view.menus.gamepopupmenus.BuildEquipments"
      styleClass="background"
      stylesheets="@../CSS/GamePopUpMenus.css"
      prefHeight="500.0" prefWidth="750.0">

    <ImageView layoutX="690" layoutY="20" onMouseClicked="#exitPopUp">
        <Image url="@../images/menus/XButton.png" requestedWidth="40" requestedHeight="40"/>
    </ImageView>
    <VBox spacing="30" alignment="CENTER" minWidth="600" layoutX="75">
        <Label styleClass="headerLabel" text="Building Equipment"/>
        <HBox alignment="CENTER" spacing="10" fx:id="equipmentHBox">

        </HBox>
    </VBox>
</Pane>
